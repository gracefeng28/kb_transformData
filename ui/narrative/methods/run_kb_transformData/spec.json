        "text_options": {
            "validate_as": "int"
        }
        }
        ,{
        "id": "max",
        "optional": true,
        "advanced": false,
        "allow_multiple": false,
        "default_values": [ ],
        "field_type": "text",
        "text_options": {
            "validate_as": "int"
        }
        }
